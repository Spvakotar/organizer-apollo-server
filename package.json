{
  "name": "graphql-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "init": "env-cmd -f ./db-push/.env prisma db push && env-cmd -f ./db-push/.env prisma db seed",
    "dev": "nodemon",
    "build": "rimraf ./build && tsc",
    "start": "npm run build && env-cmd npm run generate-client && env-cmd node build/index.js",
    "generate-client": "npx prisma generate --data-proxy",
    "echo": "url : ${DATABASE_MIGRATE_URL}",
    "migrate": "env-cmd -f ./db-push/.env npx prisma migrate deploy"
  },
  "prisma": {
    "seed": "ts-node prisma/seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.1.0",
    "apollo-server-express": "^3.10.0",
    "express": "^4.18.1",
    "graphql": "^16.5.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@types/node": "^18.0.6",
    "env-cmd": "^10.1.0",
    "nodemon": "^2.0.19",
    "prisma": "^4.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
